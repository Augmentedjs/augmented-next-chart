{
  "name": "presentation-chart",
  "version": "1.2.1",
  "description": "A simple class that renders a table like a bar chart",
  "main": "dist/presentation-chart.js",
  "scripts": {
    "dev": "clear && webpack --mode development --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "build": "clear && webpack --mode production --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "lint": "eslint ./src/index.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augmentedjs/augmented-next-chart.git"
  },
  "author": "Bob Warren <robert_warren@gap.com>",
  "license": "Apache-2.0",
  "keywords": [
    "augmentedjs",
    "presentation",
    "chart"
  ],
  "bugs": {
    "url": "https://github.com/augmentedjs/augmented-next-chart/issues"
  },
  "homepage": "https://github.com/augmentedjs/augmented-next-chart#readme",
  "dependencies": {
    "presentation-dom": "^2.8.1",
    "presentation-mediator": "^2.5.8",
    "presentation-models": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "file-loader": "^1.1.11",
    "foodoc": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.2.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
