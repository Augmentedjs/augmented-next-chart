{
  "name": "augmentedjs-next-chart",
  "version": "1.0.1",
  "description": "A simple class that renders a table like a bar chart",
  "main": "dist/augmentedjs-next-chart.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augmentedjs/chart.git"
  },
  "author": "Bob Warren <robert_warren@gap.com>",
  "license": "Apache-2.0",
  "keywords": [
    "augmentedjs",
    "presentation",
    "chart"
  ],
  "bugs": {
    "url": "https://github.com/augmentedjs/chart/issues"
  },
  "homepage": "https://github.com/augmentedjs/chart#readme",
  "dependencies": {
    "augmentedjs-next-presentation": "^2.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.10.0",
    "mocha": "^5.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "webpack-node-externals": "^1.7.2"
  }
}
